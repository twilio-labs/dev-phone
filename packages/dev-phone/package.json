{
  "name": "@twilio-labs/dev-phone",
  "description": "Twilio Dev Phone",
  "version": "1.0.0-alpha.2.5",
  "author": "Twilio @twilio",
  "bugs": {
    "url": "https://github.com/twilio-labs/comlink-dev-phone/issues"
  },
  "dependencies": {
    "@oclif/command": "^1.5.20",
    "@oclif/config": "^1.15.1",
    "@twilio-labs/serverless-api": "^5.4.0",
    "@twilio/cli-core": "^6.0.0",
    "@twilio-labs/dev-phone-ui": "^1.0.0-alpha.1",
    "express": "^4.17.1",
    "open": "^8.4.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@twilio/cli-test": "^2.1.0",
    "@types/express": "^4.17.13",
    "chai": "^4.2.0",
    "eslint": "^8.3.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-plugin-mocha": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "globby": "^11.0.0",
    "mocha": "^9.1.3",
    "nyc": "^15.0.1",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "/oclif.manifest.json",
    "/dist"
  ],
  "homepage": "https://github.com/twilio-labs/comlink-dev-phone/packages/dev-phone",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "name": "dev-phone",
    "commands": "./dist/commands",
    "bin": "twilio",
    "repositoryPrefix": "<%- repo %>/blob/<%- version %>/<%- commandPath %>",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "dev-phone": {
        "description": "Twilio Dev Phone plugin"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twilio-labs/comlink-dev-phone.git"
  },
  "scripts": {
    "build": "tsc",
    "postpack": "rm -f oclif.manifest.json",
    "XX-posttest": "eslint --ignore-path .gitignore . && npm audit",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc --check-coverage --lines 90 --reporter=html --reporter=text mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "directories": {
    "test": "test"
  }
}
